    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug current file",
            "type": "go",
            "request": "launch",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "showLog": true,
            "apiVersion": 2,
            "trace": "verbose"
        },
        {
            "name": "Debug main",
            "type": "go",
            "request": "launch",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${workspaceFolder}/main.go",
            "showLog": true,
            "apiVersion": 2,
            "trace": "verbose"
        }
    ]
}